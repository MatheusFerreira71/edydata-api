{
  "name": "template-api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --transpile-only --ignore-watch node_modules src/server.ts",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "start": "node ./dist/server.js",
    "schema:drop": "prisma migrate reset",
    "lint": "eslint src && npm run test",
    "prepare": "husky install",
    "test": "vitest"
  },
  "dependencies": {
    "@prisma/client": "^4.0.0",
    "bcryptjs": "^2.4.3",
    "celebrate": "^14.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "eslint": "^7.21.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "mysql2": "^2.2.5",
    "xml2js": "^0.4.23",
    "xmldom": "^0.6.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.10",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/xml2js": "^0.4.11",
    "@types/xmldom": "^0.1.31",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^7.0.0",
    "prettier": "^2.2.1",
    "prisma": "^4.0.0",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.3",
    "vitest": "^0.24.1"
  }
}
